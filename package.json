{
  "name": "3vl",
  "version": "0.3.4",
  "description": "Three-valued logic vectors",
  "main": "dist/index",
  "types": "dist/index.d.ts",
  "scripts": {
    "prepare": "npm run build; npm run doc",
    "mjs": "tsc -d && mv dist/index.js dist/index.mjs",
    "cjs": "tsc -m commonjs",
    "build": "npm run mjs && npm run cjs",
    "test": "jest",
    "doc": "typedoc --out dist/docs src/"
  },
  "author": "Marek Materzok",
  "license": "BSD-2-Clause",
  "devDependencies": {
    "@types/jest": "^23.3.14",
    "jest": "^26.6.3",
    "jsverify": "^0.8.4",
    "lodash": "^4.17.20",
    "ts-jest": "^26.5.0",
    "typedoc": "^0.17.8",
    "typescript": "^3.9.7"
  },
  "homepage": "https://github.com/tilk/js_3vl",
  "repository": {
    "type": "git",
    "url": "https://github.com/tilk/js_3vl.git"
  },
  "jest": {
    "transform": {
      "^.+\\.tsx?$": "ts-jest"
    },
    "testRegex": "(/__tests__/.*|(\\.|/)(test|spec))\\.(jsx?|tsx?)$",
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js",
      "jsx",
      "json",
      "node"
    ]
  }
}
